{
    "name":"百度广告过滤",
    "version":"0.0.1",
    "manifest_version":2,
    "description":"过滤百度含有广告标签的数据",
    "browser_action":{
        "default_icon":"favicon.ico"
    },
    "icons":{ 
        "16": "favicon.ico",             
        "48": "favicon.ico",            
        "128": "favicon.ico" 
    },  
    "content_scripts":[{
        "matches":["https://www.baidu.com/*"],
        "js":["jquery-3.5.0.min.js","script.js"]
    }]
}